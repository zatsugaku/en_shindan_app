{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/diagnose.html",
        "destination": "/free/diagnose.html",
        "type": 301
      },
      {
        "source": "/results/:result",
        "destination": "/free/results/:result",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/free",
        "destination": "/free/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
